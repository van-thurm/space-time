  "editor.tokenColorCustomizations": {
    "comments": "#8b8da3",
    "textMateRules": [
      {
        "name": "Comments",
        "scope": ["comment", "punctuation.definition.comment"],
        "settings": {
          "foreground": "#8b8da3",
          "fontStyle": "italic"
        }
      },
      {
        "name": "Keywords",
        "scope": ["keyword", "keyword.control", "keyword.operator.expression", "storage.type", "storage.modifier"],
        "settings": {
          "foreground": "#7aa2d4"
        }
      },
      {
        "name": "Operators",
        "scope": ["keyword.operator", "keyword.operator.assignment"],
        "settings": {
          "foreground": "#c4a882"
        }
      },
      {
        "name": "Functions",
        "scope": ["entity.name.function", "meta.function-call", "support.function"],
        "settings": {
          "foreground": "#ff9940"
        }
      },
      {
        "name": "Function Parameters",
        "scope": ["variable.parameter", "meta.parameter"],
        "settings": {
          "foreground": "#e8d5b7",
          "fontStyle": "italic"
        }
      },
      {
        "name": "Strings",
        "scope": ["string", "string.quoted", "string.template"],
        "settings": {
          "foreground": "#ffd866"
        }
      },
      {
        "name": "String Interpolation",
        "scope": ["punctuation.definition.template-expression", "punctuation.section.embedded"],
        "settings": {
          "foreground": "#ff9940"
        }
      },
      {
        "name": "Numbers",
        "scope": ["constant.numeric"],
        "settings": {
          "foreground": "#cc7a2e"
        }
      },
      {
        "name": "Constants / Booleans",
        "scope": ["constant.language", "constant.language.boolean"],
        "settings": {
          "foreground": "#cc7a2e"
        }
      },
      {
        "name": "Variables",
        "scope": ["variable", "variable.other"],
        "settings": {
          "foreground": "#e8d5b7"
        }
      },
      {
        "name": "Object Properties",
        "scope": ["variable.other.property", "variable.other.object.property", "meta.object-literal.key"],
        "settings": {
          "foreground": "#c4a882"
        }
      },
      {
        "name": "Types / Classes / Interfaces",
        "scope": ["entity.name.type", "entity.name.class", "support.type", "support.class", "entity.other.inherited-class"],
        "settings": {
          "foreground": "#7aa2d4",
          "fontStyle": "bold"
        }
      },
      {
        "name": "Decorators / Annotations",
        "scope": ["meta.decorator", "punctuation.decorator"],
        "settings": {
          "foreground": "#ff9940",
          "fontStyle": "italic"
        }
      },
      {
        "name": "Tags (HTML/JSX)",
        "scope": ["entity.name.tag", "punctuation.definition.tag"],
        "settings": {
          "foreground": "#ff9940"
        }
      },
      {
        "name": "Tag Attributes",
        "scope": ["entity.other.attribute-name"],
        "settings": {
          "foreground": "#ffd866",
          "fontStyle": "italic"
        }
      },
      {
        "name": "CSS Properties",
        "scope": ["support.type.property-name.css"],
        "settings": {
          "foreground": "#7aa2d4"
        }
      },
      {
        "name": "CSS Values",
        "scope": ["support.constant.property-value.css", "meta.property-value.css"],
        "settings": {
          "foreground": "#ffd866"
        }
      },
      {
        "name": "CSS Selectors",
        "scope": ["entity.name.tag.css", "entity.other.attribute-name.class.css", "entity.other.attribute-name.id.css"],
        "settings": {
          "foreground": "#ff9940"
        }
      },
      {
        "name": "Regex",
        "scope": ["string.regexp"],
        "settings": {
          "foreground": "#cc7a2e"
        }
      },
      {
        "name": "Escape Characters",
        "scope": ["constant.character.escape"],
        "settings": {
          "foreground": "#ff9940"
        }
      },
      {
        "name": "Import / Require",
        "scope": ["keyword.control.import", "keyword.control.export", "keyword.control.from"],
        "settings": {
          "foreground": "#4a6fa5"
        }
      },
      {
        "name": "Module Specifiers",
        "scope": ["string.quoted.module"],
        "settings": {
          "foreground": "#ffd866"
        }
      },
      {
        "name": "Punctuation",
        "scope": ["punctuation.separator", "punctuation.terminator", "punctuation.accessor"],
        "settings": {
          "foreground": "#6b7280"
        }
      },
      {
        "name": "Brackets / Braces",
        "scope": ["punctuation.definition.block", "meta.brace"],
        "settings": {
          "foreground": "#8b8da3"
        }
      },
      {
        "name": "Markdown Headings",
        "scope": ["markup.heading", "entity.name.section.markdown"],
        "settings": {
          "foreground": "#ffd866",
          "fontStyle": "bold"
        }
      },
      {
        "name": "Markdown Bold",
        "scope": ["markup.bold"],
        "settings": {
          "foreground": "#ff9940",
          "fontStyle": "bold"
        }
      },
      {
        "name": "Markdown Italic",
        "scope": ["markup.italic"],
        "settings": {
          "foreground": "#e8d5b7",
          "fontStyle": "italic"
        }
      },
      {
        "name": "Markdown Links",
        "scope": ["markup.underline.link", "string.other.link"],
        "settings": {
          "foreground": "#7aa2d4"
        }
      },
      {
        "name": "Markdown Code",
        "scope": ["markup.inline.raw", "markup.fenced_code"],
        "settings": {
          "foreground": "#cc7a2e"
        }
      },
      {
        "name": "JSON Keys",
        "scope": ["support.type.property-name.json"],
        "settings": {
          "foreground": "#7aa2d4"
        }
      },
      {
        "name": "JSON Values (strings)",
        "scope": ["string.quoted.double.json"],
        "settings": {
          "foreground": "#ffd866"
        }
      },
      {
        "name": "YAML Keys",
        "scope": ["entity.name.tag.yaml"],
        "settings": {
          "foreground": "#7aa2d4"
        }
      },
      {
        "name": "YAML Values",
        "scope": ["string.unquoted.plain.out.yaml"],
        "settings": {
          "foreground": "#ffd866"
        }
      },
      {
        "name": "This / Self",
        "scope": ["variable.language.this", "variable.language.self"],
        "settings": {
          "foreground": "#ff9940",
          "fontStyle": "italic"
        }
      }
    ]
  }
}
